{
  "compilerOptions": {
      "target": "es5",
      "module": "CommonJs",
      "outDir": "dist",
      "alwaysStrict": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "declaration": true,
      "downlevelIteration": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "jsx": "react",
      "types": [],
      "lib": ["es2015", "dom"]
  },
  "include": [
      "src/*.ts",
      "src/*.js"
  ],
  "exclude": [
      "src/*.test.ts"
  ]
}
