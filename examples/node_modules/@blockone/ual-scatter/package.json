{
  "name": "@blockone/ual-scatter",
  "version": "0.0.22",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "clean-build": "rm -rf dist && yarn build",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "@blockone/universal-authenticator-library": "^0.0.32",
    "eosjs": "20.0.0-beta2",
    "eosjs-ecc": "^4.0.4",
    "scatterjs-core": "^2.5.0",
    "scatterjs-plugin-eosjs2": "^1.2.0"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/dist"
    ],
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/runtime": "^7.2.0",
    "@blockone/tslint-config-blockone": "^1.0.0",
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  }
}
